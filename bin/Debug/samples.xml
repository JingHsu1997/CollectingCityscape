<samples>
	<!-- Urban-themed Wave Function Collapse configuration -->
	<!-- All tiles are 3024x3024 PNG images -->

	<!-- Simple Tiled Model for Urban Elements -->
	<simpletiled name="urban_elements" subset="default" width="8" height="8" periodic="false" screenshots="1">

		<!-- Road tiles - Streets, walkways, transit lines, canals, railroads -->
		<!-- Can connect to all objects -->
		<tile name="road_1" symmetry="I"/>
		<tile name="road_2" symmetry="L"/>
		<tile name="road_3" symmetry="T"/>

		<!-- Edge/Boundaries tiles -->
		<!-- Can connect to roads, districts, landmarks, spaces -->
		<tile name="edge_1" symmetry="I"/>

		<!-- District/Section tiles -->
		<!-- Can connect to roads, boundaries, spaces -->
		<tile name="district_1" symmetry="\"/>

		<!-- Nodes/Points tiles - Connection points where roads meet -->
		<!-- Can connect to user objects -->
		<tile name="node_1" symmetry="X"/>
		<tile name="node_2" symmetry="I"/>

		<!-- Landmark/Point-reference tiles -->
		<!-- Can connect to roads, districts, spaces -->
		<tile name="landmark_1" symmetry="\"/>

		<!-- Space/Gap tiles - Open spaces between structures -->
		<!-- Can connect to roads, boundaries, districts, landmarks -->
		<tile name="space_1" symmetry="\"/>

		<!-- Users tiles - Living creatures -->
		<!-- Can connect to all objects -->
		<tile name="user_1" symmetry="\"/>

		<!-- Objects tiles - Non-living things -->
		<!-- Can connect to all objects -->
		<tile name="objects_1" symmetry="\"/>
		<tile name="objects_2" symmetry="\"/>

		<!-- Adjacency rules based on your connectivity specifications -->

		<!-- Roads can connect to everything -->
		<neighbor left="road_1 1" right="road_1 3"/>
		<neighbor left="road_1 1" right="road_2 3"/>
		<neighbor left="road_1 1" right="road_3 3"/>
		<neighbor left="road_1 1" right="edge_1 3"/>
		<neighbor left="road_1 1" right="district_1 3"/>
		<neighbor left="road_1 1" right="node_1 3"/>
		<neighbor left="road_1 1" right="landmark_1 3"/>
		<neighbor left="road_1 1" right="space_1 3"/>
		<neighbor left="road_1 1" right="user_1 3"/>
		<neighbor left="road_1 1" right="objects_1 3"/>

		<!-- Road connections -->
		<neighbor left="road_2 2" right="road_1 0"/>
		<neighbor left="road_2 2" right="road_2 0"/>
		<neighbor left="road_3 2" right="road_1 0"/>
		<neighbor left="road_3 1" right="road_1 3"/>
		<neighbor left="road_3 3" right="road_1 1"/>

		<!-- Edge connects to roads, districts, landmarks, spaces -->
		<neighbor left="edge_1 1" right="road_1 3"/>
		<neighbor left="edge_1 1" right="road_2 3"/>
		<neighbor left="edge_1 1" right="district_1 3"/>
		<neighbor left="edge_1 1" right="landmark_1 3"/>
		<neighbor left="edge_1 1" right="space_1 3"/>

		<!-- District connects to roads, boundaries, spaces -->
		<neighbor left="district_1 0" right="road_1 2"/>
		<neighbor left="district_1 0" right="road_2 2"/>
		<neighbor left="district_1 0" right="edge_1 2"/>
		<neighbor left="district_1 0" right="space_1 2"/>

		<!-- Nodes connect to roads and user objects -->
		<neighbor left="node_1 0" right="road_1 2"/>
		<neighbor left="node_1 1" right="road_1 3"/>
		<neighbor left="node_1 2" right="road_1 0"/>
		<neighbor left="node_1 3" right="road_1 1"/>
		<neighbor left="node_1 0" right="road_2 2"/>
		<neighbor left="node_1 1" right="road_2 3"/>
		<neighbor left="node_1 2" right="road_3 0"/>
		<neighbor left="node_1 3" right="road_3 1"/>
		<neighbor left="node_1 0" right="user_1 2"/>

		<neighbor left="node_2 1" right="road_1 3"/>
		<neighbor left="node_2 1" right="road_2 3"/>
		<neighbor left="node_2 1" right="user_1 3"/>

		<!-- Landmark connects to roads, districts, spaces -->
		<neighbor left="landmark_1 0" right="road_1 2"/>
		<neighbor left="landmark_1 0" right="road_2 2"/>
		<neighbor left="landmark_1 0" right="district_1 2"/>
		<neighbor left="landmark_1 0" right="space_1 2"/>

		<!-- Space connects to roads, boundaries, districts, landmarks -->
		<neighbor left="space_1 0" right="road_1 2"/>
		<neighbor left="space_1 0" right="road_2 2"/>
		<neighbor left="space_1 0" right="edge_1 2"/>
		<neighbor left="space_1 0" right="district_1 2"/>
		<neighbor left="space_1 0" right="landmark_1 2"/>

		<!-- User can connect to all objects -->
		<neighbor left="user_1 0" right="road_1 2"/>
		<neighbor left="user_1 0" right="road_2 2"/>
		<neighbor left="user_1 0" right="road_3 2"/>
		<neighbor left="user_1 0" right="edge_1 2"/>
		<neighbor left="user_1 0" right="district_1 2"/>
		<neighbor left="user_1 0" right="node_1 2"/>
		<neighbor left="user_1 0" right="node_2 2"/>
		<neighbor left="user_1 0" right="landmark_1 2"/>
		<neighbor left="user_1 0" right="space_1 2"/>
		<neighbor left="user_1 0" right="objects_1 2"/>
		<neighbor left="user_1 0" right="objects_2 2"/>

		<!-- Objects can connect to all objects -->
		<neighbor left="objects_1 0" right="road_1 2"/>
		<neighbor left="objects_1 0" right="road_2 2"/>
		<neighbor left="objects_1 0" right="road_3 2"/>
		<neighbor left="objects_1 0" right="edge_1 2"/>
		<neighbor left="objects_1 0" right="district_1 2"/>
		<neighbor left="objects_1 0" right="node_1 2"/>
		<neighbor left="objects_1 0" right="node_2 2"/>
		<neighbor left="objects_1 0" right="landmark_1 2"/>
		<neighbor left="objects_1 0" right="space_1 2"/>
		<neighbor left="objects_1 0" right="user_1 2"/>
		<neighbor left="objects_1 0" right="objects_2 2"/>

		<neighbor left="objects_2 0" right="road_1 2"/>
		<neighbor left="objects_2 0" right="road_3 2"/>
		<neighbor left="objects_2 0" right="district_1 2"/>
		<neighbor left="objects_2 0" right="space_1 2"/>
		<neighbor left="objects_2 0" right="user_1 2"/>
		<neighbor left="objects_2 0" right="objects_1 2"/>

	</simpletiled>
